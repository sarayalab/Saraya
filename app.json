{
  "expo": {
    "name": "Saraya",
    "slug": "sarayaplus",
    "scheme": ["saraya"],
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./App/assets/logoAssets/sarayaiconsquare.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./App/assets/Splash_Screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saraya.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.saraya.SarayaApp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e60898d3-8a35-4a8b-a3c9-8ce68df15b62"
      }
    },
    "plugins": ["@react-native-google-signin/google-signin"],
    "owner": "rapler"
  }
}
